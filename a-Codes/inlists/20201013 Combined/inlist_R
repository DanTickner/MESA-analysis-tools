! inlist: Create a rotating star.

&star_job

 read_extra_star_job_inlist1 = .true.
 extra_star_job_inlist1_name = 'inlist_a_general'
 read_extra_star_job_inlist2 = .true.
 extra_star_job_inlist2_name = 'inlist_aa_rotation'
 
 ! Save model (required by file "rn" even if not needed itself)
 save_model_when_terminate = .true.
 save_model_filename = 'mod_R.mod'

/ ! end of star_job namelist



&controls

 read_extra_controls_inlist1 = .true.
 extra_controls_inlist1_name = 'inlist_a_general'
 read_extra_controls_inlist2 = .true.
 extra_controls_inlist2_name = 'inlist_aa_rotation'
 
 photo_directory = 'Photos_R'

/ ! end of controls namelist



&pgstar

 read_extra_pgstar_inlist1 = .true.
 extra_pgstar_inlist1_name = 'inlist_aaa_pgstar'
   
/ ! end of pgstar namelist