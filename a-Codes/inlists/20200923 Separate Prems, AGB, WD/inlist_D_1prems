 save_model_filename = 'mod_D_1prems.mod'

/ ! end of star_job namelist

&controls

 read_extra_controls_inlist1 = .true.
 extra_controls_inlist1_name = 'inlist_1prems'
 
 photo_directory = 'Photos_D'

/ ! end of controls namelist

&pgstar

 read_extra_pgstar_inlist1 = .true.
 extra_pgstar_inlist1_name = 'inlist_1prems'
   
/ ! end of pgstar namelist
